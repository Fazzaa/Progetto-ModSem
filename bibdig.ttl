@prefix : <http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix bag: <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix bibo: <https://www.dublincore.org/specifications/bibo/bibo/bibo.rdf.xml#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix list: <http://www.ontologydesignpatterns.org/cp/owl/list.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix swrla: <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix dbpedia: <http://dbpedia.org/resource/> .
@prefix sequence: <http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#> .
@prefix cpannotationschema: <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#> .
@base <http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/> .

<http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale> rdf:type owl:Ontology ;
                                                                owl:imports <http://www.ontologydesignpatterns.org/cp/owl/list.owl> ;
                                                                dc:creator "Fabio Luciani" ;
                                                                dc:date "13/06/2024" ;
                                                                dc:description "L'ontologia bibliotecaDigitale è stata pensata per modellare il dominio delle risorse testuali e non presenti sul web dal punto di vista bibliografico."@it ;
                                                                dc:title "BibliotecaDigitale"@it ;
                                                                vann:preferredNamespacePrefix "bibdig" ;
                                                                vann:preferredNamespaceUri "http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale"^^xsd:anyURI ;
                                                                rdfs:label "Ontologia per biblioteca digitale"@it .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem
bag:hasItem rdf:type owl:AnnotationProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#follows
sequence:follows rdf:type owl:AnnotationProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#precedes
sequence:precedes rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/contieneEbook
:contieneEbook rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf bag:itemContent ;
               rdfs:domain :VolumeSaga ;
               rdfs:range :E-book ;
               rdfs:comment "Collegamento tra la risorsa e il libro che contiene"@it ;
               rdfs:label "Contiene libro"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/contienePuntata
:contienePuntata rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf bag:itemContent ;
                 rdfs:domain :PuntataPodcast ;
                 rdfs:range :Podcast ;
                 rdfs:comment "Collegamento tra la risorsa e il podcast che contiene"@it ;
                 rdfs:label "Contiene puntata"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/copertinaDi
:copertinaDi rdf:type owl:ObjectProperty ;
             owl:inverseOf :haCopertina ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :Copertina ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( :AudioLibro
                                        :E-book
                                      )
                        ] ;
             rdfs:comment "Indica la copertina associata a un e-book o audiolibro specifico."@it ;
             rdfs:label "copertina di"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/haAutore
:haAutore rdf:type owl:ObjectProperty ;
          owl:inverseOf :haScritto ;
          rdfs:domain :RisorsaDigitale ;
          rdfs:range :Autore ;
          rdfs:comment "Indica l'autore di una certa risorsa digitale"@it ;
          rdfs:label "Ha autore."@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/haCasaEditrice
:haCasaEditrice rdf:type owl:ObjectProperty ;
                owl:inverseOf :haPubblicato ;
                rdfs:domain :RisorsaDigitale ;
                rdfs:range :CasaEditrice ;
                rdfs:comment "Indica l'editore di una certa risorsa digitale"@it ;
                rdfs:label "Ha casa editrice"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/haCopertina
:haCopertina rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :AudioLibro
                                         :E-book
                                       )
                         ] ;
             rdfs:range :Copertina ;
             rdfs:comment "Proprietà che definisce la copertina di un e-book o audiolibro"@it ;
             rdfs:label "ha copertina"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/haFormato
:haFormato rdf:type owl:ObjectProperty ;
           rdfs:domain :RisorsaDigitale ;
           rdfs:range :Formato ;
           rdfs:comment "Indica il formato di una certa risorsa digitale"@it ;
           rdfs:label "Ha formato"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/haGenere
:haGenere rdf:type owl:ObjectProperty ;
          rdfs:domain :RisorsaDigitale ;
          rdfs:range :Genere ;
          rdfs:comment "Indica il genere di una certa risorsa digitale"@it ;
          rdfs:label "Ha genere"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/haLingua
:haLingua rdf:type owl:ObjectProperty ;
          rdfs:domain :RisorsaDigitale ;
          rdfs:range :Lingua ;
          rdfs:comment "Indica la lingua di una certa risorsa digitale"@it ;
          rdfs:label "Ha lingua"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/haNarrato
:haNarrato rdf:type owl:ObjectProperty ;
           owl:inverseOf :haNarratore ;
           rdfs:domain :Narratore ;
           rdfs:range :RisorsaAudio ;
           rdfs:comment "Indica la risorsa audio narrata da un certo narratore"@it ;
           rdfs:label "Ha narrato"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/haNarratore
:haNarratore rdf:type owl:ObjectProperty ;
             rdfs:domain :RisorsaAudio ;
             rdfs:range :Narratore ;
             rdfs:comment "Rappresenta la relazione tra una risorsa audio e colui che l'ha narrata."@it ;
             rdfs:label "Ha narratore"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/haPubblicato
:haPubblicato rdf:type owl:ObjectProperty ;
              rdfs:domain :CasaEditrice ;
              rdfs:range :RisorsaDigitale ;
              rdfs:comment "Indica la relazione tra l'editore e la risorsa digitale che ha pubblicato"@it ;
              rdfs:label "Ha pubblicato"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/haScritto
:haScritto rdf:type owl:ObjectProperty ;
           rdfs:domain :Autore ;
           rdfs:range :RisorsaDigitale ;
           rdfs:comment "Indica la risorsa digitale scritta da un certo autore."@it ;
           rdfs:label "Ha scritto"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/scriveGenere
:scriveGenere rdf:type owl:ObjectProperty ;
              rdfs:domain :Autore ;
              rdfs:range :Genere ;
              owl:propertyChainAxiom ( :haScritto
                                       :haGenere
                                     ) ;
              rdfs:comment "Indica il genere di cui scrive un certo autore"@it ;
              rdfs:label "Scrive genere"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/scriveInLingua
:scriveInLingua rdf:type owl:ObjectProperty ;
                rdfs:domain :Autore ;
                rdfs:range :Lingua ;
                rdfs:comment "Proprietà che indica in quale Lingua ha scritto un certo Autore."@it ;
                rdfs:label "Scrive in lingua"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/stessoGenereDi
:stessoGenereDi rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ;
                rdfs:domain :RisorsaDigitale ;
                rdfs:range :RisorsaDigitale ;
                rdfs:comment "Proprietà che indica che una certa RisorsaDigitale abbia lo stesso genere di un'altra RisorsaDigitale"@it ;
                rdfs:label "Stesso genere di"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/durataInOre
:durataInOre rdf:type owl:DatatypeProperty ;
             rdfs:domain :RisorsaAudio ;
             rdfs:range xsd:decimal ;
             rdfs:comment "Indica la durata in ore di una risorsa digitale"@it ;
             rdfs:label "Durata in ore"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/fotoProfiloURL
:fotoProfiloURL rdf:type owl:DatatypeProperty ;
                rdfs:domain :Persona ;
                rdfs:range xsd:anyURI ;
                rdfs:comment "Proprietà che associa una Persona ad un URL rappresentante un'immagine di quest'ultima"@it ;
                rdfs:label "URL foto profilo"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/haAnnoPubblicazione
:haAnnoPubblicazione rdf:type owl:DatatypeProperty ;
                     rdfs:domain :RisorsaDigitale ;
                     rdfs:range xsd:gYear ;
                     rdfs:comment "Indica l'anno di pubblicazione di una certa risorsa digitale."@it ;
                     rdfs:label "Anno pubblicazione"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/haISBN
:haISBN rdf:type owl:DatatypeProperty ,
                 owl:FunctionalProperty ;
        rdfs:domain :RisorsaDigitale ;
        rdfs:range xsd:string ;
        rdfs:comment "L'International Standard Book Number, ISBN, utilizzato normalmente per identificare i libri pubblicati."@it ;
        rdfs:label "ISBN"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/haImmagineURL
:haImmagineURL rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :Copertina ;
               rdfs:range xsd:anyURI ;
               rdfs:comment "Indica l'URL di una determinata immagine, solitamente una copertina"@it ;
               rdfs:label "ha immagine URL"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/haNome
:haNome rdf:type owl:DatatypeProperty ;
        rdfs:domain owl:Thing ;
        rdfs:range xsd:string ;
        rdfs:comment "Proprietà che definisce il nome di una persona o di una organizzazione"@it ;
        rdfs:label "Ha nome"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/haTitolo
:haTitolo rdf:type owl:DatatypeProperty ;
          rdfs:domain :RisorsaDigitale ;
          rdfs:range xsd:string ;
          rdfs:comment "Indica il titolo di una risorsa digitale."@it ;
          rdfs:label "ha titolo"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/nazionalità
:nazionalità rdf:type owl:DatatypeProperty ;
             rdfs:domain :Autore ;
             rdfs:range xsd:string ;
             rdfs:comment "Indica la nazionalità di un autore"@it ;
             rdfs:label "Nazionalità"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/nomeAutore
:nomeAutore rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :haNome ;
            rdfs:domain :Autore ;
            rdfs:range xsd:string ;
            rdfs:comment "Indica il nome e il cognome dell'autore"@it ;
            rdfs:label "Nome autore"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/nomeEditore
:nomeEditore rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :haNome ;
             rdfs:domain :CasaEditrice ;
             rdfs:range xsd:string ;
             rdfs:comment "Indica il nome della casa editrice."@it ;
             rdfs:label "Nome editore"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/nomeFormato
:nomeFormato rdf:type owl:DatatypeProperty ;
             rdfs:domain :Formato ;
             rdfs:range xsd:string ;
             rdfs:comment "Definisce il nome di un formato."@it ;
             rdfs:label "nome formato"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/nomeGenere
:nomeGenere rdf:type owl:DatatypeProperty ;
            rdfs:domain :Genere ;
            rdfs:range xsd:string ;
            rdfs:comment "Definisce il nome di un genere letterario."@it ;
            rdfs:label "nome genere"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/nomeNarratore
:nomeNarratore rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :haNome ;
               rdfs:domain :Narratore ;
               rdfs:range xsd:string ;
               rdfs:comment "Indica il nome di un narratore di risorse audio"@it ;
               rdfs:label "Nome narratore"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/numeroPagine
:numeroPagine rdf:type owl:DatatypeProperty ;
              rdfs:domain :RisorsaDigitale ;
              rdfs:range xsd:positiveInteger ;
              rdfs:comment "Indica il numero di pagine di una certa risorsa digitale"@it ;
              rdfs:label "Numero di pagine"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/numeroPuntate
:numeroPuntate rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf bag:size ;
               rdfs:domain :SeriePodcast ;
               rdfs:range xsd:positiveInteger ;
               rdfs:comment "indica il numero di puntate presenti nella serie di un podcast"@it ;
               rdfs:label "Numero di puntate"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/numeroVolumi
:numeroVolumi rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf bag:size ;
              rdfs:domain :SagaDiEbook ;
              rdfs:range xsd:positiveInteger ;
              rdfs:comment "Indica il numero di volumi presenti in una saga di libri"@it ;
              rdfs:label "Numero di volumi"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/tradotto
:tradotto rdf:type owl:DatatypeProperty ;
          rdfs:domain :RisorsaDigitale ;
          rdfs:range xsd:boolean ;
          rdfs:comment "Indica se la risorsa in questione è stata tradotta o meno in altre lingue"@it ;
          rdfs:label "Tradotto"@it .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/ontology/Language
<http://dbpedia.org/ontology/Language> rdf:type owl:Class ;
                                       owl:equivalentClass :Lingua .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/ArticoloAccademico
:ArticoloAccademico rdf:type owl:Class ;
                    owl:equivalentClass bibo:AcademicArticle ;
                    rdfs:subClassOf :RisorsaDigitale ;
                    rdfs:comment "Un articolo scientifico pubblicato in una rivista accademica o conferenza, sottoposto a peer review."@it ;
                    rdfs:label "Articolo accademico"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/AudioLibro
:AudioLibro rdf:type owl:Class ;
            rdfs:subClassOf :RisorsaAudio ;
            rdfs:comment "Una registrazione audio di un libro letto ad alta voce. Gli audiolibri sono disponibili in formato digitale e possono essere ascoltati su vari dispositivi."@it ;
            rdfs:label "Audiolibro"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/Autore
:Autore rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :haScritto ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :RisorsaDigitale
                            ] ;
        rdfs:subClassOf :Persona ;
        rdfs:comment "Un individuo che ha scritto o contribuito alla creazione di una risorsa digitale nella libreria."@it ;
        rdfs:label "Autore"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/AutoreDiSaga
:AutoreDiSaga rdf:type owl:Class ;
              rdfs:subClassOf :Autore ;
              rdfs:comment "Definisce un autore che ha scritto uno o più libri facenti parte di una saga di libri"@it ;
              rdfs:label "Autore di saga"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/AutoreMultilingua
:AutoreMultilingua rdf:type owl:Class ;
                   rdfs:subClassOf :Autore ;
                   rdfs:comment "Autore che ha scritto risorse in diverse lingue"@it ;
                   rdfs:label "Autore multilingua"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/CasaEditrice
:CasaEditrice rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :haPubblicato ;
                                    owl:someValuesFrom :RisorsaDigitale
                                  ] ;
              rdfs:comment "Organizzazione che si occupa della pubblicazione di una risorsa digitale."@it ;
              rdfs:label "Casa Editrice"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/Copertina
:Copertina rdf:type owl:Class ;
           rdfs:comment "Una risorsa che rappresenta la copertina di un e-book o di un audiolibro"@it ;
           rdfs:label "Copertina"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/E-book
:E-book rdf:type owl:Class ;
        rdfs:subClassOf bibo:Book ;
        rdfs:comment "Un libro disponibile in formato digitale, che può essere letto su dispositivi elettronici come e-reader, tablet, computer e smartphone"@it ;
        rdfs:label "E-book"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/E-book_Lungo
:E-book_Lungo rdf:type owl:Class ;
              rdfs:subClassOf :E-book ;
              rdfs:comment "Un libro con almeno 500 pagine"@it ;
              rdfs:label "E-book lungo"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/Formato
:Formato rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Class ;
                               owl:oneOf ( :EPUB
                                           :MP3
                                           :PDF
                                         )
                             ] ;
         rdfs:comment "Indica il formato in cui è disponibile la risorsa digitale"@it ;
         rdfs:label "Formato"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/Genere
:Genere rdf:type owl:Class ;
        rdfs:comment "Indica una categoria di opere letterarie che condividono caratteristiche comuni in termini di stile, forma, contenuti, e tematiche"@it ;
        rdfs:label "Genere"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/Lingua
:Lingua rdf:type owl:Class ;
        rdfs:comment "Indica la lingua in cui è scritta la risorsa digitale."@it ;
        rdfs:label "Lingua"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/Narratore
:Narratore rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Restriction ;
                                 owl:onProperty :haNarrato ;
                                 owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass :RisorsaAudio
                               ] ;
           rdfs:subClassOf :Persona ;
           rdfs:comment "Persona che si occupa della narrazione di un podcast o di un audiolibro"@it ;
           rdfs:label "Narratore"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/Persona
:Persona rdf:type owl:Class ;
         rdfs:subClassOf foaf:Person ;
         rdfs:comment "Una persona rilevante per le risorse digitali della biblioteca."@it ;
         rdfs:label "Persona"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/Podcast
:Podcast rdf:type owl:Class ;
         rdfs:subClassOf :RisorsaAudio ;
         rdfs:comment "Registrazione audio, disponibile online, che tratta vari argomenti."@it ;
         rdfs:label "Podcast"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/PuntataPodcast
:PuntataPodcast rdf:type owl:Class ;
                rdfs:subClassOf list:ListItem ;
                rdfs:comment "Singola registrazione audio all'interno di una serie"@it ;
                rdfs:label "Puntata Podcast"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/RisorsaAudio
:RisorsaAudio rdf:type owl:Class ;
              rdfs:subClassOf :RisorsaDigitale ;
              owl:disjointWith bibo:Book ;
              rdfs:comment "Una risorsa disponibile in formato audio digitale. Può includere audiolibri, podcast, registrazioni di lezioni e altri tipi di contenuti audio."@it ;
              rdfs:label "Risorsa audio"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/RisorsaDigitale
:RisorsaDigitale rdf:type owl:Class ;
                 rdfs:comment "Una risorsa digitale che include articoli, libri e risorse audio come audiolibri."@it ;
                 rdfs:label "Risorsa digitale"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/SagaDiEbook
:SagaDiEbook rdf:type owl:Class ;
             rdfs:subClassOf list:List ;
             rdfs:comment "Serie di libri collegati fra loro, spesso con personaggi in comune e una trama continua."@it ;
             rdfs:label "Saga di libri"@it .


###  http://www.semanticweb.org/fazza/progetto/bibliotecaDigitale/SeriePodcast
:SeriePodcast rdf:type owl:Class ;
              rdfs:subClassOf list:List ;
